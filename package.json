    "@jupyterlab/apputils": "^1.0.0-alpha.6",
    "@jupyterlab/coreutils": "^3.0.0-alpha.6",
    "@jupyterlab/launcher": "^1.0.0-alpha.6"
  },
  "devDependencies": {
    "rimraf": "^2.6.1",
    "typescript": "~3.1.1"
  },
  "jupyterlab": {
    "extension": true
